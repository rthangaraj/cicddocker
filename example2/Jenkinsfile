pipeline {
  agent (any) 
  environment {
    name = "abc"
    loc = "dba"

  }

  stages {
    stage ("Checkout") {
      steps {
        checkout scm 

      }
	  }
	  
	  stage ("Pushing the docker Image") {
      steps {
	  script{
        docker.withRegistry('https://docker.io/rameshthangaraj', 'dockerhubcred') {
          def app = docker.build("docker.io/rameshthangaraj:pytest-image", "./example2/")
          app.push()

        }
        }
      }
    }

}

}
